{
  "name": "backend-inovia",
  "version": "1.0.0",
  "description": "Desafio Full Stack",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "build": "webpack build",
    "compose": "docker-compose down --remove-orphans && docker-compose up --build --force-recreate",
    "compose:d": "npm run compose -- -d",
    "lint": "eslint '*/**/*.{js,ts}' --fix",
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src\"",
    "test:verbose": "jest --passWithNoTests --runInBand --detectOpenHandles --verbose",
    "test": "jest --passWithNoTests --runInBand --detectOpenHandles --silent --noStackTrace",
    "test:watch": "npm run test -- --watch",
    "test:unit": "npm run test -- -c jest.unit.config.ts",
    "test:integration": "npm run test -- -c jest.integration.config.ts",
    "test:coverage": "npm run test -- --coverage",
    "test:ci:unit": "npm run test:unit -- --coverage",
    "test:ci:integration": "npm run test:integration -- --coverage",
    "test:staged": "npm test -- --findRelatedTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RegislaineRegis/backend-inovia.git"
  },
  "keywords": [
    "inovia"
  ],
  "author": "Regislaine",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RegislaineRegis/backend-inovia/issues"
  },
  "homepage": "https://github.com/RegislaineRegis/backend-inovia#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.8.0",
    "sequelize": "^6.28.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.0",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "commitizen": "^4.2.6",
    "commitlint": "^17.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jest-environment-node": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
